@{
    ViewData["Title"] = "List";
}
@model List<RecipeModel>

@{
    if (Model.Count == 0)
    {
        <h1>No recipes right now</h1>
    }
    else
    {
        @foreach (var recipe in Model)
        {
            <p >@Html.ActionLink($"{recipe.RecipeID} - {recipe.RecipeName}", "RecipeDetails", new { id = recipe.RecipeID }, null)</p>
            <p>@recipe.RecipeID - @recipe.RecipeName</p>
        }
    }

    }